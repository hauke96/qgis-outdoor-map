{
  "version": 8,
  "name": "outdoor-map-style",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "localhost": {
      "type": "vector",
      "tiles": ["http://localhost:8000/tiles/{z}/{x}/{y}.pbf"],
      "minZoom": 0,
      "maxZoom": 14,
      "minzoom": 10,
      "maxzoom": 16
    }
  },
  "sprite": "http://localhost:8000/sprites/sprite",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "rgba(255, 255, 255, 1)"}
    },
    {
      "id": "landuse_residential",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "residential", "education"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(230, 230, 230, 1)"}
    },
    {
      "id": "landuse_commercial",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": [
        "all",
        [
          "in",
          "landuse",
          "commercial",
          "industrial",
          "retail",
          "institutional",
          "garagesport ",
          "railway ",
          "religious "
        ]
      ],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(242, 224, 245, 1)"}
    },
    {
      "id": "landuse_farmland",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "farmland"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(255, 245, 215, 1)"}
    },
    {
      "id": "natural_water",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["==", "landuse", "water"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(179, 229, 252, 1)",
        "fill-outline-color": "rgba(79, 195, 247, 1)"
      }
    },
    {
      "id": "natural_forest",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "forest", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(153, 205, 149, 1)"}
    },
    {
      "id": "natural_forest-pattern",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "forest", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-pattern": "forest_pattern"}
    },
    {
      "id": "natural_grass",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": [
        "all",
        ["in", "landuse", "grass", "meadow", "grassland", "village_green"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(204, 235, 194, 1)"}
    },
    {
      "id": "natural_rock",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "bare_rock"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-pattern": "rock_pattern"}
    },
    {
      "id": "natural_shingle",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["in", "landuse", "shingle", "scree"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-pattern": "shingle_pattern"}
    },
    {
      "id": "natural_scrub",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["==", "landuse", "scrub"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(191, 220, 170, 1)"}
    },
    {
      "id": "buildings_background",
      "type": "fill",
      "source": "localhost",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "rgba(220, 214, 211, 1)",
        "fill-outline-color": "rgba(180, 180, 180, 1)"
      }
    },
    {
      "id": "road_tiny_back",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 12,
      "filter": [
        "all",
        ["in", "highway", "residential", "living_street", "pedestrian", "road"]
      ],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 2, 16, 8],
        "line-color": "rgba(158, 158, 158, 1)"
      }
    },
    {
      "id": "road_sml_back",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": [
        "all",
        ["in", "highway", "unclassified", "tertiary", "secondary", "primary"]
      ],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 4, 16, 12],
        "line-color": "rgba(158, 158, 158, 1)"
      }
    },
    {
      "id": "road_tiny_front",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 12,
      "filter": [
        "all",
        ["in", "highway", "residential", "living_street", "pedestrian", "road"]
      ],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 0.5, 16, 7],
        "line-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "road_s_front",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": ["all", ["in", "highway", "unclassified", "tertiary"]],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 1, 16, 10],
        "line-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "road_m_front",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": ["all", ["in", "highway", "secondary"]],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 1, 16, 10],
        "line-color": "rgba(255, 245, 157, 1)"
      }
    },
    {
      "id": "road_l_front",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": ["all", ["in", "highway", "primary"]],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 1, 16, 10],
        "line-color": "rgba(255, 204, 128, 1)"
      }
    },
    {
      "id": "trail_back",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": ["all", ["in", "highway", "path", "footway", "track"]],
      "layout": {
        "line-join": "round",
        "line-cap": "butt",
        "visibility": "visible"
      },
      "paint": {"line-width": 3, "line-color": "rgba(255, 255, 255, 0.5)"}
    },
    {
      "id": "trail_paths",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "minzoom": 0,
      "filter": ["all", ["in", "highway", "path", "footway"]],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {"line-width": 1.5, "line-color": "rgba(229, 57, 53, 1)"}
    },
    {
      "id": "landuse_military",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["==", "landuse", "military"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(239, 154, 154, 0.25)"}
    },
    {
      "id": "landuse_military-pattern",
      "type": "fill",
      "source": "localhost",
      "source-layer": "landuse",
      "filter": ["all", ["==", "landuse", "military"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-pattern": "dash_pattern"}
    },
    {
      "id": "track_good",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "filter": [
        "all",
        ["==", "highway", "track"],
        ["!in", "tracktype", "grade4", "grade5"]
      ],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-color": "rgba(191, 54, 12, 1)",
        "line-width": 2,
        "line-dasharray": [5, 2]
      }
    },
    {
      "id": "track_bad",
      "type": "line",
      "source": "localhost",
      "source-layer": "highway",
      "filter": [
        "all",
        ["==", "highway", "track"],
        ["in", "tracktype", "grade4", "grade5"]
      ],
      "layout": {"line-join": "round", "line-cap": "butt"},
      "paint": {
        "line-color": "rgba(191, 54, 12, 1)",
        "line-width": 2,
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "highway_label",
      "type": "symbol",
      "source": "localhost",
      "source-layer": "highway",
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["OpenSans Regular"],
        "text-size": 12
      },
      "paint": {
        "text-color": "rgba(109, 76, 65, 1)",
        "text-halo-color": "rgba(255, 255, 255, 0.75)",
        "text-halo-blur": 0,
        "text-halo-width": 2
      }
    },
    {
      "id": "buildings_foreground",
      "type": "fill",
      "source": "localhost",
      "source-layer": "building",
      "minzoom": 14,
      "layout": {"visibility": "none"},
      "paint": {
        "fill-color": "#e0e0e0",
        "fill-opacity": 0.75,
        "fill-outline-color": "rgba(189, 189, 189, 1)"
      }
    }
  ],
  "id": "5vwt2vaok"
}